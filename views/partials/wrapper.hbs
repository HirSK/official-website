<!--Carousel Wrapper-->
<div id="multi-item-example" class="carousel slide carousel-multi-item carousel-multi-item-2" data-ride="carousel">

    <!--Controls-->
    <div class="controls-top">
        <a class="black-text" href="#multi-item-example" data-slide="prev"><i class="fa fa-angle-left fa-3x pr-3"></i></a>
        <a class="black-text" href="#multi-item-example" data-slide="next"><i class="fa fa-angle-right fa-3x pl-3"></i></a>
    </div>
    <!--/.Controls-->

    <!--Slides-->
    <div class="carousel-inner" role="listbox">

              {{#each latest3}}
                <div class="carousel-item active">

                    <div class="col-md-4 mb-4">
                        <div >
                            <img class="img-fluid" src="../images/{{this.image1}}" alt="Card image cap">
                            <div class="container"  >
                                <h7><b>{{this.title}}</b></h7>
                                <br>
                                <div>
                                    <p >{{this.conclusion}}</p>
                                </div>
                            </div>
                            <div>
                                <a class="btn btn-outline-black btn-sm" href="/event/{{this.id}}" target="_blank" role="button">Read More
                                    <span class="glyphicon glyphicon-link"></span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>

              {{/each}}

                {{#each early3}}
              <div class="carousel-item ">

                  <div class="col-md-4 mb-4">
                      <div >
                          <img class="img-fluid" src="../images/{{this.image1}}" alt="Card image cap">


                          <div class="container">
                              <h7><b>{{this.title}}</b></h7>
                              <br>
                              <p >{{this.conclusion}}</p>
                          </div>
                          <div>
                              <a class="btn btn-outline-black btn-sm" href="/event/{{this.id}}" target="_blank" role="button">Read More
                                  <span class="glyphicon glyphicon-link"></span>
                              </a>
                          </div>
                      </div>
                  </div>
              </div>
            {{/each}}
    </div>
    <!--/.Slides-->

</div>
<!--/.Carousel Wrapper-->
